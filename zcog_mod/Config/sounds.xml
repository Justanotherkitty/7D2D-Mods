<mod>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'player_death_stinger']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Sad</setattribute>
	
	<append xpath="/Sounds">
		<SoundDataNode name = "Cog_Death">
			<AudioSource name="Sounds/AudioSource_Pain"/>
			<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?cog_death"/>
			<Channel name="Mouth"/>
			<Priority name="0"/>
			<maxVoicesPerEntity value="1"/>
			<LowestPitch name="0.95"/>
			<HighestPitch name="1.05"/>
			<LocalCrouchVolumeScale value="1.0"/>
			<CrouchNoiseScale value="0.5"/>
			<NoiseScale value="1"/>
			<MaxVoices value="10"/>
			<MaxRepeatRate value="0"/>
		</SoundDataNode>
	</append>
	
	<set xpath = "/Sounds/SoundDataNode[@name = 'vultureattack']">
		<AudioSource name="Sounds/AudioSource_Creatures100Meter"/>
		<Noise ID="1" noise="7" time="3" muffled_when_crouched="0.5"/>
		<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Bird_Dive"/>
		<Channel name="Mouth"/>
		<Priority name="2"/>
		<maxVoicesPerEntity value="1"/>
		<LowestPitch name="0.9"/>
		<HighestPitch name="1.0"/>
		<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/>
		<NoiseScale value="1"/>
		<MaxVoices value="10"/>
		<MaxRepeatRate value="0.9"/>
	</set>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'player_bandage']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Heal</setattribute>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'minibike_horn']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Bike_horn</setattribute>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'tooltip_popup']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?UI_notif</setattribute>
	
	<set xpath = "/Sounds/SoundDataNode[@name = 'avalanche']">
		<AudioSource name="Sounds/AudioSource_Impact"/>
		<Noise ID="4" noise="60" time="2" heat_map_strength="0.5" heat_map_time="90"/>
		<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Block_Fall_1"/>
		<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Block_Fall_2"/>
		<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Block_Fall_3"/>
		<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Block_Fall_4"/>
		<AudioClip ClipName="#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Block_Fall_5"/>
		<LocalCrouchVolumeScale value="0.5"/>
		<CrouchNoiseScale value="0.5"/>
		<NoiseScale value="1"/>
		<MaxVoices value="10"/>
		<MaxRepeatRate value="0.01"/> 
	</set>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'light_pipebomb']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Throw_Dynamite</setattribute>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'fuse_lp']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Fuse_Loop</setattribute>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'explosion1']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Explosion</setattribute>
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'explosion2']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Explosion</setattribute>
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'explosion3']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Explosion</setattribute>
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'explosion_grenade']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Explosion</setattribute>
	
	<setattribute xpath = "/Sounds/SoundDataNode[@name = 'landmine_trigger']/AudioClip" name = "ClipName">#Mods/zcog_mod/Resources/Sounds/sounds.unity3d?Mine_Trigger</setattribute>
</mod>